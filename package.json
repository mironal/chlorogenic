    "fmt": "npm run _fm -- --write",
    "fmt:check": "npm run _fm -- -l"
  },
  "devDependencies": {
    "@types/color": "3.0.0",
    "@types/jest": "23.3.3",
    "@types/node": "13.9.4",
    "@types/reach__router": "1.0.1",
    "@types/react": "16.4.16",
    "@types/react-dom": "16.0.8",
    "@types/react-loadable": "5.4.1",
    "@types/react-redux": "6.0.9",
    "@types/react-select": "2.0.4",
    "codecov": "3.1.0",
    "prettier": "1.14.3",
    "react-testing-library": "5.2.0",
    "typescript": "3.1.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/**/*.d.ts",
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  }
}

{
  "name": "chlorogenic",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@mdi/js": "2.8.94",
    "@mdi/react": "1.0.1",
    "@reach/router": "1.2.1",
    "@rematch/core": "1.0.4",
    "axios": "0.18.0",
    "color": "3.0.0",
    "firebase": "5.5.3",
    "immer": "1.7.2",
    "react": "16.5.2",
    "react-dnd": "5.0.0",
    "react-dnd-html5-backend": "5.0.1",
    "react-dom": "16.5.2",
    "react-loadable": "5.5.0",
    "react-markdown": "4.0.2",
    "react-redux": "5.0.7",
    "react-scripts-ts": "2.17.0",
    "react-select": "2.1.0",
    "styled-components": "3.4.9"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
